<script type='text/x-handlebars-template' id='main_structure'>
    <div class='container-fluid'>
        <div class='row'>
            <div class='col-xs-12 col-sm-8 col-md-8 col-lg-8'>
                <h1>
                    {{module_label}}
                </h1>
            </div>
            <div class='col-xs-12 col-sm-4 col-md-4 col-lg-4'>
                <h3 class='instructions pull-right' id='user_guide_top'>
                    <i class="fa fa-file-pdf-o"></i>{{user_guide}}
                </h3>
            </div>
        </div>
        <br>
        <div class='row'>
